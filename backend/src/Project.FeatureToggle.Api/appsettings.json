{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Program": {
    "MongoDbSettings": {
      "User": "<YOUR_USERNAME>",
      "Password": "<YOUR_PASSWORD>",
      "Cluster": "<YOUR_CLUSTER>",
      "Database": "<YOUR_DATABSE>",
      "Domain": "<YOUR_DOMAIN>",
      "CollectionName": "<YOUR_COLLECTION>"
    },
    "RedisSettings": {
      "Username": "<YOUR_USERNAME>",
      "Password": "<YOUR_PASSWORD>",
      "Endpoint": "<YOUR_ENDPOINT>",
      "Port": "<YOUR_PORT>",
      "Ssl": true,
      "AbortConnect": false,
      "ExpiresInMinutes": 60
    },
    "ElasticsearchSettings": {
      "Endpoint": "<YOUR_ENDPOINT>",
      "Username": "<YOUR_USERNAME>",
      "Password": "<YOUR_PASSWORD>",
      "IndexPattern": "<YOUR_INDEXPATTERN>" 
    },
    "SwaggerSettings": {
      "Enable": true,
      "Title": "Feature Toggle API",
      "Description": "API para gerenciamento de feature toggles",
      "Version": "v1",
      "Contact": {
        "Name": "Rafael Deroncio",
        "Email": "rafael.deroncio@empresa.com",
        "Url": "https://empresa.com"
      },
      "License": {
        "Name": "MIT",
        "Url": "https://opensource.org/licenses/MIT"
      },
      "Servers": [
        {
          "Url": "https://localhost:5226",
          "Description": "Development"
        }
      ]
    }
  }
}